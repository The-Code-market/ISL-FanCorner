<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codemaster.fancorner">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FanCorner">
        <activity android:name=".CreateAccountScreen"></activity>
        <activity
            android:name=".ChatScreen"
            android:theme="@style/Theme.FanCorner.RedStatusBar"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".OTPScreen" />
        <activity android:name=".SignInScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>